                            <div class="row price">
                                <p class="card-text1">@item.PRICE<p style="text-decoration:line-through;">(800.000)</p></p>
                                <i class="fa-solid fa-solids fa-heart like" style="color: #e03550; display: flex; margin-left: 14px;"><p style="color: black; font-family:'Times New Roman'; font-size: 13px;">@item.LIKES thích</p></i>
